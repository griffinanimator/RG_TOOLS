// edit lockhide


global proc edit_t()
{
int $tx = `checkBox -q -value tx`;
int $ty = `checkBox -q -value ty`;
int $tz = `checkBox -q -value tz`;

if ($tx + $ty + $tz == 3)
{
checkBox -e -value 1 t;
}
else
{
if ($tx + $ty + $tz == 0)
{
checkBox -e -value 0 t;
}
}
}


global proc edit_r()
{
int $rx = `checkBox -q -value rx`;
int $ry = `checkBox -q -value ry`;
int $rz = `checkBox -q -value rz`;

if ($rx + $ry + $rz == 3)
{
checkBox -e -value 1 r;
}
else
{
if ($rx + $ry + $rz == 0)
{
checkBox -e -value 0 r;
}
}
}


global proc edit_s()
{
int $sx = `checkBox -q -value sx`;
int $sy = `checkBox -q -value sy`;
int $sz = `checkBox -q -value sz`;

if ($sx + $sy + $sz == 3)
{
checkBox -e -value 1 s;
}
else
{
if ($sx + $sy + $sz == 0)
{
checkBox -e -value 0 s;
}
}
}


