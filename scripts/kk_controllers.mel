//snap

global proc snap() 	{
			string $sel_1[] = `ls -sl`;
			textField -e -text $sel_1 text01;
			
			string $text = `textField -q -text text01`;
	
	if ($text == "")
			{
			sphere;
			}
			else
			{
			sphere;
			string $sel_2[] = `ls -sl`;
			string $cons [] = `parentConstraint $sel_1 $sel_2`;
			delete $cons;
			}	
			}

// get

global proc get ()
{

string $sele [] = `ls -sl`;
string $gtx = `getAttr -lock $sele.tx`;
string $gty = `getAttr -lock $sele.ty`;
string $gtz = `getAttr -lock $sele.tz`;

string $grx = `getAttr -lock $sele.rx`;
string $gry = `getAttr -lock $sele.ry`;
string $grz = `getAttr -lock $sele.rz`;

string $gsx = `getAttr -lock $sele.sx`;
string $gsy = `getAttr -lock $sele.sy`;
string $gsz = `getAttr -lock $sele.sz`;

string $gv =  `getAttr -lock $sele.visibility`;


if ($gtx == 1)
{
checkBox -e -value 1 tx;
}
else
{
checkBox -e -value 0 tx;
}


if ($gty == 1)
{
checkBox -e -value 1 ty;
}
else
{
checkBox -e -value 0 ty;
}


if ($gtz == 1)
{
checkBox -e -value 1 tz;
}
else
{
checkBox -e -value 0 tz;
}

if ($grx == 1)
{
checkBox -e -value 1 rx;
}
else
{
checkBox -e -value 0 rx;
}

if ($gry == 1)
{
checkBox -e -value 1 ry;
}
else
{
checkBox -e -value 0 ry;
}

if ($grz == 1)
{
checkBox -e -value 1 rz;
}
else
{
checkBox -e -value 0 rz;
}

if ($gsx == 1)
{
checkBox -e -value 1 sx;
}
else
{
checkBox -e -value 0 sx;
}

if ($gsy == 1)
{
checkBox -e -value 1 sy;
}
else
{
checkBox -e -value 0 sy;
}

if ($gsz == 1)
{
checkBox -e -value 1 sz;
}
else
{
checkBox -e -value 0 sz;
}

if ($gv == 1)
{
checkBox -e -value 1 v;
}
else
{
checkBox -e -value 0 v;
}
edit_t;
edit_r;
edit_s;
}




//t
global proc ont ()
	{
	checkBox -e -value 1 tx;
       checkBox -e -value 1 ty;
       checkBox -e -value 1 tz;
	lockhide;
	}
global proc oft ()
	{
	checkBox -e -value 0 tx;
       checkBox -e -value 0 ty;
       checkBox -e -value 0 tz;
	lockhide;
        }
