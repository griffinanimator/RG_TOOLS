// Mirror ----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


global proc mirrorX()
{
string $copy = `checkBox -q -value cb01`;
	get;
	mirrorunlock;
if ($copy == 1)
	{
	duplicate;
	}

	group -n "controllers_mirror_grp";
		xform -os -piv 0 0 0;

	scale -1 1 1 ;
	
		ungroup controllers_mirror_grp;

makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
lockhide;
}

